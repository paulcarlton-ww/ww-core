{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "arn:aws:iam::482649550366:oidc-provider/"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          ":sub": "system:serviceaccount:prometheus:amp-iamproxy-query-service-account"
        }
      }
    }
  ]
}
